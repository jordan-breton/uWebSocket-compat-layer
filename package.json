{
  "name": "uws-reverse-proxy",
  "version": "3.2.1",
  "description": "uWebSocket.js based reverse proxy. It allows to run uWebSocket.js on the same port than any other HTTP server. For example: making uWebSocket.js and express/ nestjs / koa / fastify / whatever on the same port possible.",
  "keywords": [
    "uWebSockets.js",
    "express",
    "http",
    "websocket"
  ],
  "main": "index.cjs",
  "scripts": {
    "test": "nyc --reporter=text --reporter=html --report-dir=./.coverage mocha -n expose-gc './{,!(node_modules)/**}/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jordan-breton/uws-reverse-proxy"
  },
  "author": "Jordan Breton <contact@jordan-breton.com>",
  "license": "LGPL-3.0-or-later",
  "private": false,
  "devDependencies": {
    "@types/node": "^18.15.13",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "typedoc": "^0.23.28",
    "typedoc-plugin-mermaid": "^1.10.0",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^5.0.2",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.20.0"
  },
  "dependencies": {}
}
